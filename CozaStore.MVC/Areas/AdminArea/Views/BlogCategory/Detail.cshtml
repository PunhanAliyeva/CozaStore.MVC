@model BlogCategory

<div class="card">
    <div class="card-header">
        <h5>
            Bloq-Kateqoriya haqqında
        </h5>
        <br>
        <a asp-controller="blogCategory" asp-action="Index" class="btn btn-success mt-3 mb-5">Geriyə Qayıt</a>
    </div>
    <dv class="card-block">
        <div class="form-group row">
        </div>
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Ad</label>
            <div class="col-sm-10">
                <input asp-for="Name" class="form-control" disabled>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Yaradılma tarixi</label>
            <div class="col-sm-10">
                <input asp-for="CreatedAt" class="form-control" disabled>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Dəyişiklik olunma tarixi</label>
            <div class="col-sm-10">
                <input asp-for="UpdatedAt" class="form-control" disabled>
            </div>
        </div>
</div>
